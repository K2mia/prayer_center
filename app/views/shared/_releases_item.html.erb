
<li id="<%= releases_item.id %>">
  <span class='timestamp'>
    Scraped from Discogs <%= time_ago_in_words( releases_item.created_at ) + ' ago.' %>
  </span>

  <% if current_user?( releases_item.keyword.user ) %>
<table>
<tr>
<td width=80>Title:</td>
<td><%= raw(releases_item.title) %></td>
</tr><tr>
<td >Label:</td>
<td><%= releases_item.label %></td>
</tr><tr>
<td>Format:</td>
<td><%= releases_item.format %></td>
</tr><tr>
<td>Country:</td>
<td><%= releases_item.country %></td>
</tr><tr>
<td>Released:</td>
<td><%= releases_item.released %></td>
</tr><tr>
<td>Genre:</td>
<td><%= releases_item.genre %></td>
</tr><tr>
<td>Style:</td>
<td><%= releases_item.style %></td>
</tr>
</table>
  <% end %>
</li>
