
<h1>Create new Album Search Keys</h1>

<div class='row'>
  <div class='span6 offset3'>
    <%= form_for( @keyword ) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :keys %>
      <%= f.text_field :keys %>

      <%= f.hidden_field :user_id, :value => current_user.id %>

      <%= f.submit 'Create New Search Keys', class: 'btn btn-large btn-primary' %>
    <% end %>
  </div>
</div>


<%= link_to 'Back', keywords_path %>
